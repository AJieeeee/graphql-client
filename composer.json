{
    "name": "convenia/graphql-client",
    "description": "Client to make graphql requests",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
            "classmap": [
                "src/"
            ],
            "psr-4": {
                "Convenia\\GraphQLClient\\": "src/"
            }
        },
        "autoload-dev": {
            "psr-4": {
                "Convenia\\GraphQLClient\\Tests\\": "tests/"
            }
        },
        "config": {
            "preferred-install": "dist",
            "sort-packages": true,
            "optimize-autoloader": true
        },
        "prefer-stable": true,
    "authors": [
        {
            "name": "Tauan Lemos",
            "email": "tauan.lemos@convenia.com.br"
        }
    ]
}
